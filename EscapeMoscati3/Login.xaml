<Page x:Class="EscapeMoscati3.Login"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EscapeMoscati3"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Login">

    <Grid Background="White">
        <Label Content="Nome" Margin="318,45,378,370"/>
        <TextBox x:Name="Nome" TextWrapping="Wrap" Text="TextBox" Width="120" Margin="318,85,362,330" TextChanged="Nome_TextChanged"/>
        <Label Content="Password" Margin="318,165,378,250"/>
        <TextBox x:Name="Password" TextWrapping="Wrap" Text="TextBox" Width="120" Margin="318,205,362,160" TextChanged="Password_TextChanged"/>


    </Grid>
</Page>
